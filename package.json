    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx tsx src/index.ts",
    "scanner": "npx tsx src/scanner-runner.ts",
    "walkthrough": "npx tsx src/walkthrough.ts",
    "bg-scan": "ENABLE_SERVER=false npx tsx src/scanner-runner.ts",
    "prepare-bg": "npx tsx src/scripts/prepareBackgroundDb.ts",
    "generate-schemas": "npx tsx schema/generateSchemas.ts",
    "generate-api-docs": "npx tsx schema/generateApiDocs.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:reset": "ts-node src/scripts/resetPostgres.ts",
    "compare-stores": "DATA_STORE=hybrid npx tsx src/scripts/compareStores.ts",
    "db:migrate-from-redis": "DATA_STORE=postgres npx tsx src/scripts/migrateRedisToPostgres.ts",
    "db:export-sql": "DATA_STORE=postgres npx tsx src/scripts/exportPostgresData.ts",
    "db:backup": "npx tsx src/scripts/backupPostgres.ts"
